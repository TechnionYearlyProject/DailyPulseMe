set ns [new Simulator]
source tb_compat.tcl

set nodeA [$ns node]
tb-set-node-os $nodeA VirtualBox5.1
tb-set-node-startcmd $nodeA "/share/VBguests/startup/runsuperveda.sh"

set hacker [$ns node]
tb-add-node-attribute $hacker containers:node_type openvz
tb-add-node-attribute $hacker containers:openvz_template ubuntu-10.04-x86

$ns run